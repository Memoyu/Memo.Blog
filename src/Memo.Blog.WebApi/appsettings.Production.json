{
  "ConnectionStrings": {
    "MySql": "{mysql-conn}",
    "Redis": "{redis-conn}"
  },
  "Mongo": {
    "ConnectionString": "{mongodb-conn}",
    "Database": "blog"
  },
  "CorsOrigins": "http://*.memoyu.com,http://localhost:11011,http://localhost:11012",

  "Authorization": {
    "Jwt": {
      "Secret": "{jwt-secret}",
      "Issuer": "memoyu",
      "Audience": "memo.blog",
      "ExpiryInMin": 720
    },
    "Qiniu": {
      "AK": "{qiniu-ak}",
      "SK": "{qiniu-sk}",
      "Bucket": "memoyu-blog",
      "Host": "http://oss.blog.memoyu.com/",
      "UseHttps": false
    },
    "GitHub": {
      "Owner": "Memoyu",
      "Token": "{github-token}"
    }
  }
}
