{
  "ConnectionStrings": {
    "MySql": "{mysql-conn}",
    "Redis": "{redis-conn}"
  },
  "Mongo": {
    "ConnectionString": "mongodb://{mongodb-conn}",
    "Database": "blog"
  },
  "CorsOrigins": "http://blog.memoyu.com,http://blog.admin.memoyu.com,http://localhost:11011,http://localhost:11012",

  "Authorization": {
    "Jwt": {
      "Secret": "{jwt-secret}",
      "Issuer": "memoyu",
      "Audience": "memo.blog",
      "ExpiryInMin": 720
    },
    "Qiniu": {
      "AK": "{qiniu-ak}",
      "SK": "{qiniu-sk}",
      "Bucket": "memoyu-blog",
      "Host": "http://oss.blog.memoyu.com/",
      "UseHttps": false
    },
    "GitHub": {
      "Owner": "Memoyu",
      "Token": "{github-token}"
    },
    "Mail": {
      "Enable": true,
      "DisplayName": "Memo博客系统通知",
      "Email": "{mail-email}",
      "Password": "{mail-password}",
      "Host": "smtp-mail.outlook.com",
      "Port": 587,
      "EnableSsl": true
    }
  },
  "AppSettings": {
    "ClientDomain": "http://blog.memoyu.com/"
  }
}
